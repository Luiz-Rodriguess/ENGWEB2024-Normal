extends layout

block content
    .w3-card-4
        header.w3-container.w3-teal
            h1 Entidade #{id}
            p
                a(href="/") Página Principal
        .w3-container
            table.w3-table-all
                tr
                    th Contrato
                each contrato in listaDeContratos
                    tr
                        td=contrato.idcontrato

        p 
            b Preço Total:
            =preco